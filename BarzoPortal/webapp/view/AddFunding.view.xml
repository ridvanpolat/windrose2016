<mvc:View controllerName="Barzo.controller.AddFunding" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<App id="app">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<core:Fragment fragmentName="Barzo.view.addingFindingInfo" type="XML"/>
				</content>
				<footer>
					<Toolbar>
						<ToolbarSpacer/>
						<Button type="Accept" text="{i18n>Save}" press="onSave"/>
						<Button type="Reject" text="{i18n>Clear}" press="onClear"/>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>